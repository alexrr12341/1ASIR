Ejercicio 3)
CREATE TABLE COFRADIAS
(
	codigo VARCHAR2(2),
	nombrecorto VARCHAR2(40),
	nombrereal VARCHAR2(150),
	sedesocial VARCHAR2(25) unique,
	sedecanonica VARCHAR2(40),
	anyofundacion NUMBER(4),
	diaprocesion VARCHAR2(20) not null,
	numhermanos NUMBER(4),
	hermanomayor VARCHAR2(25),
	CONSTRAINT pk_cofradias PRIMARY KEY(codigo),
	CONSTRAINT nomb_corto_init CHECK(nombrecorto=INITCAP(nombrecorto)),
	CONSTRAINT nomb_real_mays CHECK(nombrereal=UPPER(nombrereal)),
	CONSTRAINT sede_cano_ok CHECK(sedecanonica LIKE '%Capilla%' OR sedecanonica LIKE '%Parroquia%'),
	CONSTRAINT don_hermano_myr CHECK(SUBSTR(hermanomayor,1,2)='D.')
);

CREATE TABLE PASOS
(
	nombre VARCHAR2(35),
	tipo VARCHAR2(10),
	numcostaleros NUMBER(3),
	numnazarenos NUMBER(4),
	colorcapa VARCHAR2(10),
	colorantifaz VARCHAR2(10),
	codigocofradia VARCHAR2(2),
	CONSTRAINT pk_pasos PRIMARY KEY(nombre),
	CONSTRAINT tipo_ok CHECK(tipo='Misterio' OR tipo='Palio'),
	CONSTRAINT numcost_ok CHECK(numcostaleros BETWEEN 1 AND 150),
	CONSTRAINT numnaza_ok CHECK(numnazarenos < 1000),
	CONSTRAINT colorcapa_ok CHECK(colorcapa IN ('Blanco','Negro','Morado')),
	CONSTRAINT colorantifaz_ok CHECK(colorantifaz NOT IN ('Rosa','Amarillo')),
	CONSTRAINT fk_cofradia FOREIGN KEY (codigocofradia) REFERENCES cofradias(codigo)
);

CREATE TABLE HORARIOS
(
	codigocofradia VARCHAR2(2),
	fechahora DATE,
	calle VARCHAR2(25),
	CONSTRAINT pk_horarios PRIMARY KEY(codigocofradia, fechahora),
	CONSTRAINT fk_cofr2 FOREIGN KEY (codigocofradia) REFERENCES cofradias,
	CONSTRAINT fecha_ok_horarios CHECK(TO_CHAR(fechahora, 'MM/YYYY')='03/2013')
);
