Ejercicio 2)

Create table equipos
(
	CodEquipo Varchar2(4),
	Nombre Varchar2(30) not null,
	Nacionalidad Varchar2(20),
	NombreDirector Varchar2(20),
	constraint pk_equipos primary key(CodEquipo),
	constraint director_mayus check (NombreDirector = upper(NombreDirector))	
);

Create table ciclistas
(
	Dorsal Number(3),
	Nombre Varchar2(30) not null,
	Nacionalidad Varchar2(30),
	CodEquipo Varchar2(4),
	FechaNacimiento DATE,
	constraint pk_ciclistas primary key(Dorsal),
	constraint fk_codequipo foreign key(CodEquipo) REFERENCES equipos(CodEquipo),
	constraint dorsal_ok check (Dorsal between 1 and 189)
);

Create table etapas
(
	NumEtapa Number(2),
	NumKms Number(3),
	Tipo Varchar2(30),
	Fecha DATE,
	constraint pk_etapas primary key (NumEtapa),
	constraint fecha_ok check (to_char(Fecha,'YYYY/MM/DD') between '2014/07/27' and '2014/08/19')
);

Create table Clasificacionesetapas
(
	NumEtapa Number(2) not null,
	Dorsal Number(3) not null,
	Posicion Varchar2(8),
	DistanciaAlGanador Varchar2(8),
	constraint fk_clasificacion_etapa foreign key (NumEtapa) references etapas(NumEtapa),
	constraint fk_clasificacion_dorsal foreign key (Dorsal) references ciclistas(Dorsal),
	constraint pk_clasificaciones primary key(NumEtapa,Dorsal),
	constraint posicion_ok check((Posicion='Abandono') or (Posicion between 1 and 189))
);

